{
    "name": "kata/php",
    "description": "PHP Kata skeleton",
    "keywords": ["kata","TDD"],
    "homepage": "https://github.com/carlosbuenosvinos/php-kata",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "5.5.*@dev",
        "mockery/mockery": "dev-master@dev",
        "phpspec/phpspec": "3.0.*@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Buenosvinos",
            "email": "hi@carlos.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kata\\": "src/",
            "Kata\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "beberlei/assert": "dev-master"
    }
}
